<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<TextView android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="@string/addProject"
		android:textSize="25sp" android:textStyle="bold" android:gravity="center" />
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_width="match_parent" android:layout_height="wrap_content">
		<TextView android:id="@+id/textView1" android:layout_height="wrap_content"
			android:textSize="20sp" android:textStyle="bold"
			android:layout_width="wrap_content" android:text="@string/idProject"
			android:padding="5sp" android:width="145sp"></TextView>
		<EditText android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_weight="1"
			android:id="@+id/txtIdProject" android:inputType="number"></EditText>
	</LinearLayout>
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_width="match_parent" android:layout_height="wrap_content">
		<TextView android:id="@+id/textView1" android:layout_height="wrap_content"
			android:textSize="20sp" android:textStyle="bold"
			android:layout_width="wrap_content" android:padding="5sp"
			android:text="@string/nameProject" android:width="145sp"></TextView>
		<EditText android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_weight="1"
			android:inputType="textPersonName" android:id="@+id/txtNameProject"></EditText>
	</LinearLayout>
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_width="match_parent" android:layout_height="wrap_content">
		<TextView android:id="@+id/textView1" android:layout_height="wrap_content"
			android:textSize="20sp" android:textStyle="bold"
			android:layout_width="wrap_content" android:padding="5sp"
			android:text="@string/beginDateProject" android:width="145sp"></TextView>
		<EditText android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_weight="1"
			android:id="@+id/txtBeginDateProject" android:inputType="date"></EditText>
	</LinearLayout>
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_width="match_parent" android:layout_height="wrap_content">
		<TextView android:id="@+id/textView1" android:layout_height="wrap_content"
			android:textSize="20sp" android:textStyle="bold"
			android:layout_width="wrap_content" android:padding="5sp"
			android:text="@string/endDateProject" android:width="145sp"></TextView>
		<EditText android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_weight="1"
			android:id="@+id/txtEndDateProject" android:inputType="date"></EditText>
	</LinearLayout>
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_width="match_parent" android:layout_height="wrap_content">
		<TextView android:id="@+id/textView1" android:layout_height="wrap_content"
			android:textSize="20sp" android:textStyle="bold"
			android:layout_width="wrap_content" android:padding="5sp"
			android:text="@string/descriptionProject" android:width="145sp"></TextView>
		<EditText android:layout_height="wrap_content"
			android:layout_weight="1" android:layout_width="wrap_content"
			android:id="@+id/txtDescriptionProject" android:inputType="text"></EditText>
	</LinearLayout>
	<LinearLayout android:id="@+id/linearLayout2"
		android:layout_width="match_parent" android:layout_height="wrap_content">
		<Button android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/btnAceptar"
			android:text="@string/add" android:textSize="18sp"
			android:layout_marginTop="20sp" android:layout_marginLeft="50sp"
			android:onClick="onAddClick"></Button>
		<Button android:id="@+id/button2" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="@string/cancel"
			android:textSize="18sp" android:layout_marginTop="20sp"
			android:layout_marginLeft="40sp" android:onClick="onCancelClick"></Button>
	</LinearLayout>
	<LinearLayout android:layout_width="match_parent"
		android:layout_height="wrap_content" android:id="@+id/linearLayout3">
	</LinearLayout>
</LinearLayout>
