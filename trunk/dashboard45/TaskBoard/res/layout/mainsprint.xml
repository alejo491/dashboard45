<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent">
	<TextView android:id="@+id/textView1" android:layout_width="match_parent"
		android:layout_height="wrap_content" android:text="SPRINT"
		android:textSize="25sp" android:textStyle="bold" android:gravity="center"></TextView>
	<LinearLayout android:layout_height="wrap_content"
		android:id="@+id/linearLayout1" android:layout_width="match_parent"
		android:orientation="vertical">
		<Button android:onClick="onClickAdd" android:textSize="20sp"
			android:textStyle="normal" android:layout_marginLeft="100sp"
			android:layout_height="wrap_content" android:text="@string/addSprint"
			android:id="@+id/btnAdd" android:layout_width="124dp"
			android:layout_marginTop="30sp"></Button>
	</LinearLayout>
	<LinearLayout android:layout_height="wrap_content"
		android:id="@+id/linearLayout1" android:layout_width="match_parent"
		android:orientation="vertical">
		<Button android:textSize="20sp" android:textStyle="normal"
			android:layout_marginLeft="100sp" android:layout_height="wrap_content"
			android:layout_width="124dp" android:text="@string/modifySprint"
			android:onClick="onClickModify" android:layout_marginTop="30sp"
			android:id="@+id/btnModify"></Button>
	</LinearLayout>
	<LinearLayout android:layout_height="wrap_content"
		android:id="@+id/linearLayout1" android:layout_width="match_parent"
		android:orientation="vertical">
		<Button android:textSize="20sp" android:textStyle="normal"
			android:layout_marginLeft="100sp" android:layout_height="wrap_content"
			android:layout_width="124dp" android:id="@+id/btnView"
			android:onClick="onClickView" android:text="@string/viewSprint"
			android:layout_marginTop="30sp"></Button>
	</LinearLayout>

</LinearLayout>
